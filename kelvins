
public class Kelvins extends Celcius {
double temperature; 
	
 Kelvins() {
	
	}
 Kelvins(double newTemperature) {
		temperature = newTemperature;
	}

	
		
	double Conversion() {
		return (temperature - 32) * 5/9 + 273.15;
	}
	void set(double newTemperature) {
		temperature = newTemperature;
	}
		
	/**
	 * @param args
	 */
	
	}


